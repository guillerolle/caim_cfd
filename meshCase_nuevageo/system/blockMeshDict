FoamFile
{
    version     4.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

xMin    -0.005;
xMax    0.0975;
yMin    -0.01625;
yMax    0.011615392523609741;
zMin    -0.011609431831376672;
zMax    0.011609431831376672;
cellsX  103;
cellsY  28;
cellsZ  24;

vertices
(
    ( $xMin  $yMin  $zMin)
    ( $xMax  $yMin  $zMin)
    ( $xMax  $yMax  $zMin)
    ( $xMin  $yMax  $zMin)
    ( $xMin  $yMin  $zMax)
    ( $xMax  $yMin  $zMax)
    ( $xMax  $yMax  $zMax)
    ( $xMin  $yMax  $zMax)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) ($cellsX $cellsY $cellsZ) simpleGrading (1 1 1)
);

