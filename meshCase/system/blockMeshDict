FoamFile
{
    version     4.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

xMin    -0.005;
xMax    0.065;
yMin    -0.009644686799835074;
yMax    0.009668640082296977;
zMin    -0.009662647916230413;
zMax    0.009662647916230413;
cellsX  70;
cellsY  20;
cellsZ  20;

vertices
(
    ( $xMin  $yMin  $zMin)
    ( $xMax  $yMin  $zMin)
    ( $xMax  $yMax  $zMin)
    ( $xMin  $yMax  $zMin)
    ( $xMin  $yMin  $zMax)
    ( $xMax  $yMin  $zMax)
    ( $xMax  $yMax  $zMax)
    ( $xMin  $yMax  $zMax)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) ($cellsX $cellsY $cellsZ) simpleGrading (1 1 1)
);

edges
(
);

boundary
(
);

